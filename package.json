    "dist:prod": "esbuild --bundle --platform=node --target=node14 --external:ws --external:express --external:yaml --external:node-pty --external:portfinder --external:node-mjpeg-proxy --external:ios-device-lib --external:@dead50f7/adbkit --external:tslib --external:h264-converter --external:tinyh264 --external:xterm --external:xterm-addon-attach --external:xterm-addon-fit --external:sylvester.js --external:buffer --external:mkdirp --external:recursive-copy --external:rimraf --external:file-loader --external:svg-inline-loader --external:worker-loader --external:html-webpack-plugin --external:mini-css-extract-plugin --external:css-loader --external:ts-loader --external:@dead50f7/generate-package-json-webpack-plugin --external:@typescript-eslint/eslint-plugin --external:@typescript-eslint/parser --external:eslint --external:eslint-config-prettier --external:eslint-plugin-prettier --external:eslint-plugin-progress --external:prettier --external:cross-env --external:ts-node --external:esbuild --external:webpack-cli --external:webpack-node-externals --external:@types/bluebird --external:@types/dom-webcodecs --external:@types/express --external:@types/node --external:@types/node-forge --external:@types/npmlog --external:@types/webpack-node-externals --external:@types/ws --external:xterm-addon-attach --external:xterm-addon-fit --external:xterm-addon-attach --external:xterm-addon-fit",
    "dist": "npm run dist:prod",
    "start": "npm run dist && cd dist && npm start",
    "script:dist:start": "node ./index.js",
    "lint": "eslint src/ --ext .ts",
    "format": "eslint src/ --fix --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sergey Volkov <drauggres@gmail.com>",
  "license": "MIT",
